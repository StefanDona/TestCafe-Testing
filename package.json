{
  "name": "testcafe-testing",
  "version": "1.0.0",
  "description": "Automated tests for eCommerce store using TestCafe",
  "main": "index.js",
  "scripts": {
    "test:headed": "npx testcafe chrome tests --base-url=https://practicesoftwaretesting.com",
    "test:headless": "npx testcafe tests --base-url=https://practicesoftwaretesting.com",
    "test:html": "npx testcafe tests --base-url=https://practicesoftwaretesting.com --reporter html:results/output.html"
  },
  "author": "Stefan",
  "license": "ISC",
  "devDependencies": {
    "chance": "1.1.12",
    "dotenv": "^17.2.0",
    "prettier": "^3.6.2",
    "testcafe": "3.7.2",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-junit": "^3.0.2"
  }
}
